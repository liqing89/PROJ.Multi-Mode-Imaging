<?xml version="1.0" encoding="utf-8"?>
<ndm>
	<head>
		<target_type>TK</target_type>
		<!--xml文件描述的目标类型(飞机,舰船) [必填]-->
		<imaging_type>sar</imaging_type>
		<!--xml文件描述的成像方式(可见光,高光谱,sar) [必填]-->
		<collection></collection>
		<!--采集单位-->
		<product_level>L1</product_level>
		<!--卫星数据产品等级-->
	</head>
	<body>
		<target_info>
			<!--图像切片中的目标信息-->
			<target_id></target_id>
			<!--目标编号[必填]-->
			<target_name></target_name>
			<nation></nation>
			<!--目标国别-->
			<level_1_class>坦克</level_1_class>
			<!--飞机类[必填]-->
			<level_2_class>军用坦克</level_2_class>
			<!--军用飞机，民用飞机[必填]-->
			<level_3_class>作战坦克</level_3_class>
			<!--作战飞机，作战支援飞机，直升机，客机，其它[必填]-->
			<level_4_class>主站坦克</level_4_class>
			<!--作战飞机(战斗机，轰炸机，攻击机，前线轰炸机，反潜机，其他)；作战支援飞机(侦察机，预警机，电子战飞机，加油机，运输机，其他)[必填]-->
			<target_model></target_model>
			<!--目标型号[必填]-->
			<military_base></military_base>
			<!--所属军事基地[必填]-->
			<base_coordinate></base_coordinate>
			<!--所属军事基地坐标[必填]-->
			<motion>停靠</motion>
			<!--目标活动状态(停靠，飞行)[必填]-->
			<background_material>沥青</background_material>
			<!--目标背景:草地、水泥、沥青等-->
			<mask_mode>无遮挡</mask_mode>
			<!--遮蔽状态:弱遮挡、半遮挡、全遮挡-->
			<aspect_angle></aspect_angle>
			<!--目标方位角[必填]-->
			<indenti_grade></indenti_grade>
			<!--目标辨识等级(发现、识别、确认、描述)-->
			<pic_path></pic_path>
			<!--目标图像资料-->
			<service_state>在役</service_state>
			<!--目标在役状态-->
			<model_path></model_path>
			<!--目标三维模型-->
			<tk_length></tk_length>
			<!--机长 民用747-8-76.4军用歼10-16.43[必填]-->
			<tk_width></tk_width>
			<!--翼展民用747-8-68.5军用歼10-9.75[必填]-->
			<tk_height></tk_height>
			<!--高度民用747-8-19.5军用歼10-5.43[必填]-->
			<engine_num></engine_num>
			<!--发动机数量-->
			<engine_pos></engine_pos>
			<!--发动机位置-->
		</target_info>
		<image_info>
			<!--目标影像元信息-->
			<scene_id></scene_id>
			<!--景标识-->
			<chip_id></chip_id>
			<!--切片编号[必填]-->
			<chip_name></chip_name>
			<!--切片名称[必填]-->
			<chip_path></chip_path>
			<!--切片切片-->
			<military_base_chip_path></military_base_chip_path>
			<!--基地切片-->
			<satellite></satellite>
			<!--卫星型号-->
			<orbit_type></orbit_type>
			<!--轨道类型(低轨，中高轨，高轨)-->
			<orbit_direction></orbit_direction>
			<!--轨道方向(升轨，降轨)-->
			<sensor_type>sar</sensor_type>
			<!--传感器类型-->
			<imaging_time></imaging_time>
			<!--成像时间(yyyy-MM-dd hh:mm:ss) [必填]-->
			<resolution></resolution>
			<!--图像分辨率(米)[必填]-->
			<position>
				<!--卫星位置-->
				<sat_longitude></sat_longitude>
				<!--经度(度)-->
				<sat_latitude></sat_latitude>
				<!--纬度(度)-->
				<sat_height></sat_height>
				<!--高度(度)-->
			</position>
			<velocity>
				<!--卫星速度-->
				<sat_north></sat_north>
				<!--正北方向速度(km/h)-->
				<sat_east></sat_east>
				<!--正东方向速度(km/h)-->
				<sat_vertical></sat_vertical>
				<!--垂直方向速度(m/min)-->
			</velocity>
			<attitude>
				<!--卫星姿态-->
				<sat_viewing_angle></sat_viewing_angle>
				<!--卫星方位角(度)-->
				<sat_pitch_angle></sat_pitch_angle>
				<!--卫星俯仰角(度)-->
				<sat_yaw_angle></sat_yaw_angle>
				<!--卫星偏转角(度)-->
			</attitude>
			<relation>
				<!--卫星目标入射关系-->
				<center_angle></center_angle>
				<!--中心视角-->
				<incident_angle></incident_angle>
				<!--入射角[必填]-->
				<incident_direction></incident_direction>
				<!--入射方向[必填]-->
			</relation>
			<sar_payload>
				<!--卫星SAR成像载荷信息(在成像方式为SAR成像时，此字段有效)-->
				<imaging_method>cs</imaging_method>
				<!--图像生成方法-->
				<imaging_mode>STRIPMAP</imaging_mode>
				<!--成像模式（条带/聚束/扫描/滑动聚束）-->
				<range_resolution></range_resolution>
				<!--雷达距离向分辨率(米)-->
				<azimuth_resolution></azimuth_resolution>
				<!--雷达方位向分辨率(米)-->
				<wave_band></wave_band>
				<!--成像波段(C,L,X,P等)-->
				<polar_mode></polar_mode>
				<!--雷达极化方式(HH,HV,VH,VV)-->
			</sar_payload>
			<opt_payload>
				<!--卫星光学成像载荷信息(在成像方式为可见光,高光谱等光学成像时，此字段有效)-->
				<imaging_mode></imaging_mode>
				<!--成像模式（凝视/推扫/。。。）-->
				<GSD></GSD>
				<!--像元分辨率(米)-->
				<spectral_res></spectral_res>
				<!--光谱分辨率(纳米)-->
				<solar_zenith></solar_zenith>
				<!--太阳高度角-->
				<sun_azimuth></sun_azimuth>
				<!--太阳方位角-->
				<wave_band></wave_band>
				<!--成像谱段-->
			</opt_payload>
		</image_info>
		<inversion_info>
			<!--反演特性信息-->
			<retrieval_length></retrieval_length>
			<!--反演长度-->
			<retrieval_width></retrieval_width>
			<!--反演宽度-->
			<area></area>
			<!--面积（平方米）[必填]-->
			<aspect_angle></aspect_angle>
			<!--方位角-->
			<incidence_angle></incidence_angle>
			<!--入射角-->
			<radiance></radiance>
			<!--可见光辐射亮度-->
			<temperature></temperature>
			<!--红外反演温度-->
			<electromagnetic_scattering_characteristic>
				<!--电磁散射特性-->
				<RCS></RCS>
				<!--单站RCS-->
				<HRRP_path></HRRP_path>
				<!--一维距离像-->
				<HRRP_peak></HRRP_peak>
				<!--一维距离像极值-->
				<attribute_scatter></attribute_scatter>
				<!--属性散射中心参数-->
				<pol_scatter></pol_scatter>
				<!--极化散射参数-->
			</electromagnetic_scattering_characteristic>
		</inversion_info>
		<check_info>
			<!--审核信息-->
			<annotation_user></annotation_user>
			<!--标注用户ID-->
			<review_user></review_user>
			<!--审核用户ID-->
			<annotation_time></annotation_time>
			<!--标注时间(yyyy-MM-dd hh:mm:ss)-->
			<review_time></review_time>
			<!--审核时间(yyyy-MM-dd hh:mm:ss)-->
			<status>审核通过</status>
			<!--未审核,审核通过,审核未通过[必填]-->
			<annotation>格式完整,审核合格</annotation>
			<!--审核注释信息，如：审核未通过原因等-->
			<application_direction>适用方向</application_direction>
			<!--适用方向[必填]-->
			
		</check_info>
	</body>
</ndm>
